class reversenumber{
    public static void main(String []a){
        int numb=121,rev=0,temp=numb;
        while(numb!=0){
            rev=(rev*10)+(numb%10);
        numb=numb/10;
        }
       String result=(rev==temp)?"palindrome":"not palindrome";
        System.out.println(result);
    }
}
